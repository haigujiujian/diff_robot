          <min>0.10</min>
          <max>10.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="gazebo_rplidar">
        <topicName>/scan</topicName>
        <frameName>laser_link</frameName>
      </plugin>
    </sensor>
  </gazebo>
  <!-- imu -->
  <joint name="imu_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0          0 0.125"/>
    <parent link="base_link"/>
    <child link="imu_link"/>
  </joint>
  <!-- Create laser reference frame -->
  <link name="imu_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz=" 0 0 0 "/>
      <geometry>
        <box size="0.01 0.04 0.04"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.04 0.04"/>
      </geometry>
    </collision>
  </link>
  <!-- IMU plugin for 'body_link' -->
  <gazebo reference="imu_link">
    <gravity>true</gravity>
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>
      <topic>__default_topic__</topic>
      <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
        <topicName>imu</topicName>
        <updateRateHZ>100.0</updateRateHZ>
        <gaussianNoise>0.0</gaussianNoise>
        <xyzOffset>0 0 0</xyzOffset>
        <rpyOffset>0 0 0</rpyOffset>
        <frameName>imu_link</frameName>
      </plugin>
      <pose>0 0 0 0 0 0</pose>
    </sensor>
  </gazebo>
  <!--aubo_i10 -->
  <joint name="pedestal_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0          0 0.125"/>
    <parent link="lift2_link"/>
    <child link="pedestal_link"/>
  </joint>
  <link name="pedestal_Link">
    <visual>
      <geometry>
        <box size="1.5 1.5 0.1"/>
      </geometry>
      <material name="">
        <color rgba="0.96 0.96 0.96 1"/>
      </material>
    </visual>
  </link>
  <!--/////////////////////////////// pedestal_JOINIT ////////////////////////////////////-->
  <!--joint name="pedestal_joint" type="fixed">
    <origin xyz="0 0 0.051" rpy="0 0 0" />
    <parent link="pedestal_Link" />
    <child link = "lift2_link" />
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
  </joint-->
  <!-- /////////////////////////////////   gazebo color   ////////////////////////////////////// -->
  <gazebo reference="pedestal_Link">
    <material>Gazebo/Grey</material>
  </gazebo>
  <mbot_base_gazebo/>
</robot>

